{
  "name": "vehicle-shop",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -c=production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage --silent",
    "test:changed": "ng test --onlyChanged",
    "test:watch": "jest --watch",
    "jest:clear": "jest --clearCache",
    "test:ci": "jest --runInBand --coverage --reporters=default --reporters=jest-junit --silent",
    "postinstall": "ngcc",
    "lint": "ng lint",
    "syncscript": "node sync.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.4",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/router": "15.0.4",
    "rxjs": "7.5.0",
    "tslib": "2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.4",
    "@angular/cli": "15.0.4",
    "@angular/compiler-cli": "15.0.4",
    "@ngneat/spectator": "13.0.0",
    "@types/jest": "29.2.5",
    "jest": "28.1.3",
    "jest-junit": "15.0.0",
    "jest-location-mock": "1.0.9",
    "jest-preset-angular": "12.2.3",
    "jest-sonar-reporter": "2.0.0",
    "ng-mocks": "14.5.1",
    "typescript": "4.8.2"
  },
  "jest-junit": {
    "outputDirectory": "./junit"
  },
  "jestSonar": {
    "reportPath": "coverage/sonarqube-report",
    "reportFile": "sonarqube-report.xml",
    "indent": 4
  }
}
